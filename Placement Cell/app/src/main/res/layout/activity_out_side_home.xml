<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ecf0f3"
    tools:context=".OutSideStudents.OutSideHome">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <soup.neumorphism.NeumorphCardView
            android:id="@+id/pres"
            style="@style/Widget.Neumorph.CardView"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_margin="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:neumorph_backgroundColor="#ecf0f3"
            app:neumorph_inset="12dp"
            app:neumorph_lightSource="leftBottom"
            app:neumorph_shadowColorDark="@color/white"
            app:neumorph_shadowColorLight="@color/white"
            app:neumorph_shadowElevation="5dp"
            app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
            app:neumorph_shapeType="pressed">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|center_vertical">

                <ImageView
                    android:id="@+id/viewEvents"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:padding="30dp"
                    android:src="@drawable/planner" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/viewEvents"
                    android:layout_marginStart="15dp"
                    android:text="View Events"
                    android:textColor="@color/black" />
            </RelativeLayout>


        </soup.neumorphism.NeumorphCardView>

        <soup.neumorphism.NeumorphCardView
            android:id="@+id/med"
            style="@style/Widget.Neumorph.CardView"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@id/pres"
            app:neumorph_backgroundColor="#ecf0f3"
            app:neumorph_inset="12dp"
            app:neumorph_lightSource="leftBottom"
            app:neumorph_shadowColorDark="@color/white"
            app:neumorph_shadowColorLight="@color/white"
            app:neumorph_shadowElevation="5dp"
            app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
            app:neumorph_shapeType="pressed">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|center_vertical">

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/logout"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:alpha=".7"
                    app:lottie_autoPlay="true"
                    app:lottie_loop="true"
                    app:lottie_rawRes="@raw/logout"
                    app:lottie_speed="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/logout"
                    android:layout_centerHorizontal="true"
                    android:text="Logout"
                    android:textColor="@color/black" />
            </RelativeLayout>

        </soup.neumorphism.NeumorphCardView>

